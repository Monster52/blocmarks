<div class="row">
  <div class="col-md-12">
  <h1><%= current_user.username %> Bookmarks</h1>

    <% @bookmarks.each do |bookmark| %>
      <%= link_to bookmark.url, "http://#{bookmark.url}", class: "btn btn-warning", target: "_blank" %>
    <% end %>
    <hr>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <h3><%= current_user.username %> <em>Liked</em> Bookmarks</h3>

    <% @likes_bookmarks.each do |bookmark| %>
      <%= link_to bookmark.url, "http://#{bookmark.url}", class: "btn btn-primary", target: "_blank" %>
      <%= render partial: 'likes/like', locals: { bookmark: bookmark } %>

    <% end %>


  </div>
</div>
